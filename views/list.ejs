<%- include("header") -%> 

  <div class="box" id="heading">
    <h1><%= listTitle %></h1>
  </div>

  <div class="box">
    <%for (let i in items){%>
    <div class="item">
      <input type="checkbox">
      <p><%= items[i] %></p>
    </div>
    <% }%>

    <form class="item" action="/" method="post">
      <div>
        <input name="item" type="text" placeholder="New Item" autocomplete="off">
        <button type="submit" name="list" value=<%= listTitle %>>+</button>
      </div>
    </form>
  </div>

<%- include("footer") -%> 